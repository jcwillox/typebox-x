{
  "name": "@jcwillox/typebox-x",
  "version": "0.0.0-dev",
  "description": "Tools for working with TypeBox",
  "author": "Josh Willox <joshwillox@gmail.com> (https://github.com/jcwillox/)",
  "license": "MIT",
  "repository": "jcwillox/typebox-x",
  "type": "module",
  "keywords": [
    "nestjs",
    "typebox",
    "typescript",
    "validation",
    "openapi"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --minify",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --cache --max-warnings=0 --ext js,cjs,mjs,jsx,ts,tsx",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check ."
  },
  "dependencies": {
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@nestjs/common": "10.4.1",
    "@nestjs/swagger": "7.4.0",
    "@sinclair/typebox": "0.33.9",
    "@swc/core": "1.7.22",
    "@tsconfig/node20": "20.1.4",
    "@types/node": "20.16.3",
    "@types/validator": "13.12.1",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "npm:eslint-plugin-i@2.29.1",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "prettier": "3.3.3",
    "rxjs": "7.8.1",
    "tsup": "8.2.4",
    "tsx": "4.19.0",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@sinclair/typebox": ">=0.33.0 <1.0.0",
    "rxjs": "^7.8.1"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/swagger": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./formats": {
      "import": {
        "types": "./dist/formats.d.ts",
        "default": "./dist/formats.js"
      },
      "require": {
        "types": "./dist/formats.d.cts",
        "default": "./dist/formats.cjs"
      }
    },
    "./nestjs": {
      "import": {
        "types": "./dist/nestjs.d.ts",
        "default": "./dist/nestjs.js"
      },
      "require": {
        "types": "./dist/nestjs.d.cts",
        "default": "./dist/nestjs.cjs"
      }
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": "^18 || ^20"
  }
}
